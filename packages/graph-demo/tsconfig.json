{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "strict": true,
    "target": "es5",
    "module": "esnext",
    "allowJs": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": ["webpack-env"],
    "paths": {
      // "@/*": ["src/*"],
      // "@datafe/graph-vue": ["../../packages/graph-vue"],
      // "@datafe/graph-core": ["../../packages/graph-core/src"]
    },
    "lib": ["esnext", "dom", "dom.iterable", "scripthost"]
  }
  // "include": ["src"]
}
